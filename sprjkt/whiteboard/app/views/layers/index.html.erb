<p id="notice"><%= notice %></p>

<h1>Listing Layers</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Drawdata</th>
      <th>Z val</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @layers.each do |layer| %>
      <tr>
        <td><%= layer.user %></td>
        <td><%= layer.drawdata %></td>
        <td><%= layer.z_val %></td>
        <td><%= link_to 'Show', layer %></td>
        <td><%= link_to 'Edit', edit_layer_path(layer) %></td>
        <td><%= link_to 'Destroy', layer, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Layer', new_layer_path %>
